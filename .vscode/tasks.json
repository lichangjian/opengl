{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "gcc",
            "args": [
                "${file}",
                "-std=c99",
                "-o bin/a.exe",
                "-g"
            ]
        },
        {
            "label": "ass",
            "type": "shell",
            "command": "gcc",
            "args": [
                "${file}",
                "-S"
            ]
        }
    ]
}